
  <!DOCTYPE html>
  <html lang = "en">
  <meta charset="UTF-8">
  <head>
  <title>Hello World</title>
  </head>
  <body>
    {{$all := .}}
    <h1>Animals</h1>
    {{range $all.Animals}}
    <h2>{{.Kind}} - {{.Food}}</h2>
    {{end}}

    <h1>Fruits</h1>
    {{range $all.Fruits}}
    <h2>{{.Kind}} - {{.Quantity}}</h2>
    {{end}}
  </body>
  </html>
